{
  "version": "2.0.0",
  "tasks": [
    {
      "label": ".NET: Build",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "build",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always"
      },
      "problemMatcher": "$msCompile"
    },
    {
      "label": ".NET: Test",
      "command": "dotnet",
      "type": "shell",
      "args": ["test", "--settings", ".runsettings"],
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always"
      },
      "problemMatcher": "$msCompile"
    },
    {
      "label": ".NET: Clean",
      "command": "dotnet",
      "type": "shell",
      "args": ["clean"],
      "group": "build",
      "presentation": {
        "reveal": "always"
      }
    }
  ]
}
